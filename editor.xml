<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
  </component>
  <component name="LiveEditOptions" restartIfHotSwapFailed="true" updatePolicy="AUTO" />
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\bsstodo\b.*">
      <option name="FOREGROUND" value="73bf" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bssq\b.*">
      <option name="FOREGROUND" value="b000dc" />
      <option name="BACKGROUND" value="fdff00" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff15" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
    <pattern icon="question" useCustomColors="true" case-sensitive="true" pattern="\bSSM\b.*">
      <option name="FOREGROUND" value="760c" />
      <option name="BACKGROUND" value="fffd02" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="ff0014" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
    <filter name="SS TODO">
      <pattern index="2" />
    </filter>
    <filter name="SSQ">
      <pattern index="3" />
    </filter>
    <filter name="SSM">
      <pattern index="4" />
    </filter>
  </component>
</application>